{
  "name": "@sprucelabs/log",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "version": "10.2.208",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "description": "Logger built for the Spruce Experience Platform",
  "keywords": [
    "node",
    "log",
    "logger",
    "sprucebot",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit",
  "bugs": {
    "url": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build.umd && npm run build.node",
    "build.node": "tsc",
    "build.browser": "tsc --project tsconfig.browser.json",
    "build.umd": "rollup -c rollup.config.js && mkdir -p build/umd && cd build && sed s/index.js.map/log.js.map/ index.js > umd/log.js && mv index.js.map umd/log.js.map",
    "watch": "chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'npm run build'",
    "clean": "rm -rf build/",
    "lint": "eslint '**/*.ts' && tsc -p . --noEmit",
    "lint.fix": "eslint --fix '**/*.ts'",
    "test": "mocha -r ts-node/register/transpile-only --exit 'tests/**/*Tests*'",
    "release": "semantic-release",
    "arkit": "arkit -f index.ts -o docs/images/arkit.svg",
    "arkit.png": "arkit -f index.ts -o docs/images/arkit.png",
    "upgrade.packages": "yarn upgrade --latest && rm yarn.lock | true && yarn | true && yarn lint.fix | true",
    "upgrade.packages.all": "yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build && yarn test"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@sprucelabs/semantic-release": "^4.0.5",
    "@types/chai": "^4.2.12",
    "@types/faker": "^5.1.0",
    "@types/mocha": "^8.0.3",
    "arkit": "^1.6.4",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "eslint-config-spruce": "^10.9.42",
    "faker": "^5.1.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "rollup": "^2.26.11",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
